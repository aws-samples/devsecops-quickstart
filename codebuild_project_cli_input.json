{
    "name": "devsecops-deploy-project",
    "description": "Deploys DevSecOps-Quickstart project into your account",
    "source": {
        "type": "CODECOMMIT",
        "buildspec": "buildspec_deploy.yml", 
        "location": "https://git-codecommit.eu-west-1.amazonaws.com/v1/repos/devsecops-quickstart"
    },
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/standard:5.0",
        "computeType": "BUILD_GENERAL1_SMALL",
        "privilegedMode": true
    },
    "artifacts": {
        "type": "NO_ARTIFACTS"
    },
    "serviceRole": "arn:aws:iam::870960670466:role/devsecops-deploy-role"
}